<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:customControls="clr-namespace:AppTest.Views"
             x:Class="AppTest.Views.CalendarPage" BackgroundColor="#E7E8D1" Title="Calendar View">
  
    
    <ContentPage.Content>
        <Grid HorizontalOptions="FillAndExpand" >
            <Grid.RowDefinitions>
                <RowDefinition Height="*"></RowDefinition>
            </Grid.RowDefinitions>
            
            <StackLayout  VerticalOptions="Start" Grid.Row="0" >
                <customControls:HorizontalCalendarPage ></customControls:HorizontalCalendarPage>
            </StackLayout>
       
        </Grid>
    </ContentPage.Content>
</ContentPage>